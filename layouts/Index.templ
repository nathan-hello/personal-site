package layouts

templ IndexLayout(header templ.Component, meta templ.Component) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			@meta
		</head>
		<body class="font-[Terminus]">
			<img
				class="fixed top-0 left-0 w-full h-full z-[-1] bg-no-repeat bg-cover"
				style="background-image: url(/images/carpark.png);"
				role="presentation"
			/>
			@header
			<br/>
			{ children... }
		</body>
		<div class="h-96 bg-transparent"></div>
	</html>
}
