---
import { globBlogs, type RGlobBlogs } from '@utils/glob'
import type { GetStaticPaths, GetStaticPathsResult } from 'astro'
import NathanLayout from '@layouts/nathan/Root.astro'

export const getStaticPaths: GetStaticPaths = async () => {
    const g = await globBlogs(undefined, 'nathan', false)
    // console.log(g)
    return g
}

const props = Astro.props as RGlobBlogs['props']
---

<NathanLayout details={props.c}>
    <props.c.Component />
</NathanLayout>
